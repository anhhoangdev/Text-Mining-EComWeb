    "components": {
        "schemas": {
            "AccessTokenModel": {
                "required": [
                    "expire",
                    "value"
                ],
                "type": "object",
                "properties": {
                    "value": {
                        "minLength": 1,
                        "pattern": "^[A-Za-z0-9-_=]+\\.[A-Za-z0-9-_=]+\\.?[A-Za-z0-9-_.+/=]*$",
                        "type": "string"
                    },
                    "expire": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "additionalProperties": false
            },
            "CheckRequestAuthenticationModel": {
                "required": [
                    "code",
                    "email"
                ],
                "type": "object",
                "properties": {
                    "email": {
                        "minLength": 1,
                        "type": "string",
                        "format": "email"
                    },
                    "code": {
                        "minLength": 1,
                        "pattern": "[0-9]{6}",
                        "type": "string"
                    }
                },
                "additionalProperties": false
            },
            "GetAccessByRefreshRequestTokenModel": {
                "required": [
                    "refreshToken"
                ],
                "type": "object",
                "properties": {
                    "refreshToken": {
                        "type": "string",
                        "format": "uuid"
                    }
                },
                "additionalProperties": false
            },
            "GetByIdPublicAccountModel": {
                "required": [
                    "email",
                    "fullName"
                ],
                "type": "object",
                "properties": {
                    "email": {
                        "minLength": 1,
                        "type": "string",
                        "format": "email"
                    },
                    "fullName": {
                        "minLength": 1,
                        "type": "string"
                    },
                    "isActive": {
                        "type": "boolean",
                        "nullable": true
                    }
                },
                "additionalProperties": false
            },
            "GetByLoginTokenModel": {
                "required": [
                    "access"
                ],
                "type": "object",
                "properties": {
                    "access": {
                        "$ref": "#/components/schemas/AccessTokenModel"
                    },
                    "refresh": {
                        "$ref": "#/components/schemas/RefreshTokenModel"
                    }
                },
                "additionalProperties": false
            },
            "GetLastNameByIdAccountModel": {
                "required": [
                    "lastName"
                ],
                "type": "object",
                "properties": {
                    "lastName": {
                        "minLength": 1,
                        "type": "string"
                    }
                },
                "additionalProperties": false
            },
            "LoginRequestAccountModel": {
                "required": [
                    "clientId",
                    "password"
                ],
                "type": "object",
                "properties": {
                    "clientId": {
                        "minLength": 1,
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "minLength": 1,
                        "type": "string",
                        "format": "password"
                    }
                },
                "additionalProperties": false
            },
            "RefreshTokenModel": {
                "required": [
                    "expire",
                    "value"
                ],
                "type": "object",
                "properties": {
                    "value": {
                        "type": "string",
                        "format": "uuid"
                    },
                    "expire": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "additionalProperties": false
            },
            "RegisterRequestAccountModel": {
                "required": [
                    "email",
                    "password",
                    "repeatPassword"
                ],
                "type": "object",
                "properties": {
                    "email": {
                        "minLength": 1,
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "maxLength": 64,
                        "minLength": 6,
                        "type": "string",
                        "format": "password"
                    },
                    "repeatPassword": {
                        "maxLength": 64,
                        "minLength": 6,
                        "type": "string",
                        "format": "password"
                    }
                },
                "additionalProperties": false
            },
            "ResetPasswordRequestAccountModel": {
                "required": [
                    "auth",
                    "newPassword"
                ],
                "type": "object",
                "properties": {
                    "auth": {
                        "$ref": "#/components/schemas/CheckRequestAuthenticationModel"
                    },
                    "newPassword": {
                        "maxLength": 64,
                        "minLength": 6,
                        "type": "string",
                        "format": "password"
                    }
                },
                "additionalProperties": false
            },
            "SendRequestAuthenticationModel": {
                "required": [
                    "email"
                ],
                "type": "object",
                "properties": {
                    "email": {
                        "minLength": 1,
                        "type": "string",
                        "format": "email"
                    }
                },
                "additionalProperties": false
            }
        },
