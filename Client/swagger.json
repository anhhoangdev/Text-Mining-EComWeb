  "paths": {
      "/WeatherForecast": {
          "get": {
              "tags": [
                  "WeatherForecast"
              ],
              "operationId": "GetWeatherForecast",
              "responses": {
                  "200": {
                      "description": "Success",
                      "content": {
                          "text/plain": {
                              "schema": {
                                  "type": "array",
                                  "items": {
                                      "$ref": "#/components/schemas/WeatherForecast"
                                  }
                              }
                          },
                          "application/json": {
                              "schema": {
                                  "type": "array",
                                  "items": {
                                      "$ref": "#/components/schemas/WeatherForecast"
                                  }
                              }
                          },
                          "text/json": {
                              "schema": {
                                  "type": "array",
                                  "items": {
                                      "$ref": "#/components/schemas/WeatherForecast"
                                  }
                              }
                          }
                      }
                  }
              }
          }
      }
  },
  "components": {
      "schemas": {
          "DateOnly": {
              "type": "object",
              "properties": {
                  "year": {
                      "type": "integer",
                      "format": "int32"
                  },
                  "month": {
                      "type": "integer",
                      "format": "int32"
                  },
                  "day": {
                      "type": "integer",
                      "format": "int32"
                  },
                  "dayOfWeek": {
                      "$ref": "#/components/schemas/DayOfWeek"
                  },
                  "dayOfYear": {
                      "type": "integer",
                      "format": "int32",
                      "readOnly": true
                  },
                  "dayNumber": {
                      "type": "integer",
                      "format": "int32",
                      "readOnly": true
                  }
              },
              "additionalProperties": false
          },
          "DayOfWeek": {
              "enum": [
                  0,
                  1,
                  2,
                  3,
                  4,
                  5,
                  6
              ],
              "type": "integer",
              "format": "int32"
          },
          "WeatherForecast": {
              "type": "object",
              "properties": {
                  "date": {
                      "$ref": "#/components/schemas/DateOnly"
                  },
                  "temperatureC": {
                      "type": "integer",
                      "format": "int32"
                  },
                  "temperatureF": {
                      "type": "integer",
                      "format": "int32",
                      "readOnly": true
                  },
                  "summary": {
                      "type": "string",
                      "nullable": true
                  }
              },
              "additionalProperties": false
          }
